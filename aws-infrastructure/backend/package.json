{
  "name": "ai-research-assistant-backend-aws",
  "version": "1.0.0",
  "description": "Backend API for AI Research Assistant on AWS with EKS, RDS, DynamoDB, and more",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "ai",
    "research",
    "assistant",
    "claude",
    "aws",
    "eks",
    "rds",
    "dynamodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@aws-sdk/client-dynamodb": "^3.679.0",
    "@aws-sdk/client-s3": "^3.679.0",
    "@aws-sdk/client-secrets-manager": "^3.679.0",
    "@aws-sdk/client-sns": "^3.679.0",
    "@aws-sdk/client-sqs": "^3.679.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.679.0",
    "@aws-sdk/lib-dynamodb": "^3.679.0",
    "@aws-sdk/s3-request-presigner": "^3.679.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.54.2",
    "@opentelemetry/auto-instrumentations-node": "^0.51.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.54.2",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.54.2",
    "@opentelemetry/resources": "^1.28.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^8.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.5",
    "dotenv": "^16.4.7",
    "zod": "^3.24.1",
    "uuid": "^11.0.5",
    "pg": "^8.14.0",
    "ioredis": "^5.4.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
